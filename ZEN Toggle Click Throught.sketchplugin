// Toggle Click Through (command shift d)
@import 'common.js'

var doc = context.document
var artboard = [[doc currentPage] currentArtboard]
var root = artboard.children()
var groups = filterGroups(root)

var CT

for (var i=0;i<groups.length;++i) {

	if (groups[i].hasClickThrough() == 0) {
		CT = 1
	} else {
		CT = 0
	}
	groups[i].setHasClickThrough(CT)

}
